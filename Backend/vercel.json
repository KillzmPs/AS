{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
    "DB_HOST": "@DB_HOST", 
    "DB_USER": "@DB_USER",
    "DB_PASS": "@DB_PASS",
    "DB_PORT": "@DB_PORT",
    "DB_DATABASE": "@DB_DATABASE",
    "FA_EMAIL": "@FA_EMAIL",
    "FA_PASS": "@FA_PASS"
  }
}

  